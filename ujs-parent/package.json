{
  "name": "ujs",
  "productName": "ujs",
  "version": "1.0.0",
  "description": "ujs",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package && xcopy datas out\\my-new-app-win32-x64\\datas\\ /E /H /C /I",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Lunuy",
    "email": "luminuy@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ujs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/settingPage/index.html",
                  "js": "./src/settingPage/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "css-loader": "^4.3.0",
    "electron": "10.1.5",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "node-loader": "^1.0.2",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@electron-forge/core": "^6.0.0-beta.54",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/fs-extra": "^9.0.3",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/ncp": "^2.0.4",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/socket.io": "^2.1.11",
    "axios": "^0.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csstype": "^3.0.4",
    "debug": "^4.2.0",
    "edit-json-file": "^1.5.0",
    "electron-fs-extra": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "eval-safe": "^1.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "http": "0.0.1-security",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0",
    "node": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sanitize-html": "^2.3.0",
    "socket.io": "^2.3.0",
    "ts-node": "^9.0.0"
  }
}
